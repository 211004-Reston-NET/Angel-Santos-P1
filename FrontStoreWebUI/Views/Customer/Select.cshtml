@{
}

@model IEnumerable<FrontStoreWebUI.Models.CustomerVM>

<h3 class="display-4"> Customer Credentials </h3>




<table class="table table-striped table-hover">
    <tr>
        <th>First Name</th>
        <th> Last Name</th>
        <th> Address</th>
        <th> Email</th>
        <th> Phone </th>

    </tr>
    @foreach (var i in Model)
    {
        <tr>


            <td>@Html.DisplayFor(model => i.FirstName)</td>
            <td>@Html.DisplayFor(model => i.LastName)</td>
            <td>@Html.DisplayFor(model => i.StreetAddress)</td>
            <td>@Html.DisplayFor(model => i.Email)</td>
            <td>@Html.DisplayFor(model => i.Phone)</td>
            

        </tr>
    }
    </table>
